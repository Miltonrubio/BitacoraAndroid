<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AlertDialogCustomStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@drawable/roundedbackground</item>
    </style>

    <style name="EditTextNoBorder">
        <item name="android:background">@null</item>
        <!-- Otros atributos de estilo que desees aplicar -->
    </style>

    <style name="NavigationViewOficina" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:background">@color/vino</item>
        <item name="itemIconTint">@color/white</item>
        <item name="itemIconSize">24dp</item>
    </style>


    <style name="NavigationViewComun" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:background">@color/naranjita</item>
        <item name="itemIconTint">@color/white</item>
        <item name="itemIconSize">24dp</item>
    </style>






    <style name="RoundedTextInputLayout" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">15dp</item>
    </style>



    <style name="editTexts">
        <item name="android:background">@drawable/input_redondo</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:textSize">16sp</item>
    </style>

</resources>